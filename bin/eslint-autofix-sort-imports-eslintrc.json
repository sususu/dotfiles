{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "flowtype",
    "import"
  ],
  "rules": {
    "sort-imports": ["warn"]
  }
}
