" キーマップ用のプレフィックス定義

let mapleader = "\<Space>"

" For utility
nnoremap <Leader> <Nop>
vnoremap <Leader> <Nop>

" For unite
nnoremap [unite] <Nop>
nmap s [unite]
nnoremap [unite-ex] <Nop>
nmap sx [unite-ex]

" For typescript
nnoremap [tsc] <Nop>
nmap <Leader>t [tsc]

" For jshint
nnoremap [jshint] <Nop>
nmap <Leader>h [jshint]

