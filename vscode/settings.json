{
  "relativePath.ignore": [
    "**/node_modules/**",
    "**/*.dll",
    "**/obj/**",
    "**/objd/**",
    "**/tmp/**",
    "**/coverage/**"
  ],
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "json.schemas": [
    {
      "fileMatch": ["package.json"],
      "url": "http://json.schemastore.org/package"
    },
    {
      "fileMatch": ["babelrc.json", ".babelrc"],
      "url": "http://json.schemastore.org/babelrc"
    },
    {
      "fileMatch": [".eslintrc", "eslint.json", "eslint.js"],
      "url": "http://json.schemastore.org/eslintrc"
    },
    {
      "fileMatch": [".stylelintrc"],
      "url": "http://json.schemastore.org/stylelint"
    },
    {
      "fileMatch": ["manifest.json"],
      "url": "http://json.schemastore.org/chrome-manifest"
    }
  ],
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "dist": true,
    "build": true,
    "tmp": true
  },
  "breadcrumbs.enabled": true,
  "editor.formatOnSave": true,
  "editor.renderIndentGuides": true,
  "editor.rulers": [80, 100],
  "editor.tabSize": 2,
  "eslint.enable": false,
  "eslint.packageManager": "yarn",
  "eslint.workingDirectories": [],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": false,
  "gitlens.codeLens.scopes": ["document"],
  "gitlens.historyExplorer.enabled": true,
  "html.format.extraLiners": "",
  "html.format.indentInnerHtml": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoEnable": false,
  "npm.bin": "yarn",
  "npm.enableScriptExplorer": true,
  "npm.includeDirectories": ["frontend"],
  "prettier.disableLanguages": ["vue", "markdown"],
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "python.pythonPath": "python3",
  "sync.anonymousGist": false,
  "sync.askGistName": false,
  "sync.autoDownload": false,
  "sync.autoUpload": false,
  "sync.forceDownload": false,
  "sync.gist": "9db5bac4a2030867479baf23811529ba",
  "sync.host": "",
  "sync.lastDownload": "2018-05-07T07:54:49.066Z",
  "sync.lastUpload": "2018-08-22T07:29:59.115Z",
  "sync.pathPrefix": "",
  "sync.quietSync": false,
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "tslint.enable": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.useSystemClipboard": true,
  "vsicons.projectDetection.disableDetect": true,
  "window.nativeTabs": true,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Visual Studio Dark",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.showTabs": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.renderWhitespace": "boundary"
}
